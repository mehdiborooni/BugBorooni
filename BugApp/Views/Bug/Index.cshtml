@*@model IList<BugApp.Models.Bug>*@
@model IEnumerable<BugApp.Models.Bug>
<h2>BugIndex</h2>
<hr />
<a asp-action="Create" asp-controller="Bug" class="btn btn-dark">Create</a>
<hr />

@foreach (var bug in Model)
{
<p>
    @bug.Id
    @bug.Name
    <a asp-action="Details" asp-route-id="@bug.Id" asp-controller="Bug" class="btn btn-info">Details</a>
    <a asp-action="Edit" asp-route-id="@bug.Id" asp-controller="Bug" class="btn btn-primary">Edit</a>
    <a asp-action="Delete" asp-route-id="@bug.Id" asp-controller="Bug" class="btn btn-danger">Delete</a>

</p>
}